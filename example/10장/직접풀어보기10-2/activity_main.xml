<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv1"></ImageView>

    <ImageView
        android:id="@+id/iv3"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv2"></ImageView>

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv1"></ImageView>

    <ImageView
        android:id="@+id/iv7"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv4"></ImageView>

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic5"
        app:layout_constraintStart_toEndOf="@+id/iv4"
        app:layout_constraintTop_toBottomOf="@+id/iv2"></ImageView>

    <ImageView
        android:id="@+id/iv8"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic8"
        app:layout_constraintStart_toEndOf="@+id/iv7"
        app:layout_constraintTop_toBottomOf="@+id/iv5"></ImageView>

    <ImageView
        android:id="@+id/iv6"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic6"
        app:layout_constraintStart_toEndOf="@+id/iv5"
        app:layout_constraintTop_toBottomOf="@+id/iv3"></ImageView>

    <ImageView
        android:id="@+id/iv9"
        android:layout_width="130dp"
        android:layout_height="160dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/pic9"
        app:layout_constraintStart_toEndOf="@+id/iv8"
        app:layout_constraintTop_toBottomOf="@+id/iv5"></ImageView>

    <android.widget.Button
        android:id="@+id/btnFinish"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="투표종료"
        app:layout_constraintTop_toBottomOf="@+id/iv7"></android.widget.Button>

</androidx.constraintlayout.widget.ConstraintLayout>